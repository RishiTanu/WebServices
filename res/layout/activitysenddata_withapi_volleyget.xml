<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Volley.Sendget_request">


    <TextView
        android:id="@+id/bid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_18sdp"
        android:layout_marginTop="@dimen/_40sdp"
        android:layout_centerHorizontal="true"
        android:text="BookingId" />

    <TextView
        android:id="@+id/bokdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_18sdp"
        android:layout_marginTop="@dimen/_70sdp"
        android:layout_centerHorizontal="true"
        android:text="BookingDate" />

    <TextView
        android:id="@+id/dismbno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_18sdp"
        android:layout_marginTop="@dimen/_100sdp"
        android:layout_centerHorizontal="true"
        android:text="DistributorMbNo" />

    <TextView
        android:id="@+id/stdmbno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_130sdp"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/_18sdp"
        android:text="StudioMbNo" />

    <TextView
        android:id="@+id/pass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="190dp"
        android:textSize="@dimen/_18sdp"
        android:layout_centerHorizontal="true"
        android:text="password"
     />

    <TextView
        android:id="@+id/disitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="230dp"
        android:textSize="@dimen/_18sdp"
        android:layout_centerHorizontal="true"
        android:text="Display Title" />

    <ImageView
        android:id="@+id/fpage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_180sdp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="250dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/bpage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="@dimen/_18sdp"
        android:layout_marginBottom="250dp"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/stdpassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_280sdp"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/_18sdp"
        android:text="Studio Password" />

    <TextView
        android:id="@+id/stdname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/_122sdp"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/_18sdp"
        android:text="StudioName" />
</RelativeLayout>